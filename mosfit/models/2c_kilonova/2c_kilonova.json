{
    "@references":[
        {
            "bibcode": "2017arXiv170708132V"
        },
        {
            "bibcode": "2017arXiv171011576V"
        }
    ],
    "ebv":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "inputs":[
            "transient"
        ],
        "latex":"E({\\rm B} - {\\rm V})"
    },
    "frad":{
        "kind":"parameter",
        "value":0.999,
        "class":"parameter",
        "latex":"f_{\\rm Ni}"
    },
    "nhhost":{
        "kind":"parameter",
        "value":1.0e14,
        "class":"parameter",
        "latex":"n_{\\rm H,host}",
        "derived_keys":"avhost"
    },
    "redshift":{
        "kind":"parameter",
        "value":0.009727,
        "class":"parameter",
        "latex":"z"
    },
    "lumdist":{
        "kind":"parameter",
        "value":43.36,
        "class":"parameter",
        "latex":"d"
    },
    "texplosion":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "latex":"t_{\\rm exp}\\,{\\rm (days)}",
        "requests":{
            "min_times": "transient"
        }
    },
    "temperature_red":{
        "kind":"parameter",
        "value":100,
        "class":"parameter",
        "latex":"T_{\\minr}\\,{\\rm (K)}"
    },
    "temperature_blue":{
        "kind":"parameter",
        "value":100,
        "class":"parameter",
        "latex":"T_{\\minb}\\,{\\rm (K)}"
    },
    "kappa_red":{
        "kind":"parameter",
        "value":10.0,
        "class":"parameter",
        "latex":"\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappa_blue":{
        "kind":"parameter",
        "value":0.5,
        "class":"parameter",
        "latex":"\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappagamma":{
        "kind":"parameter",
        "value":100.0,
        "class":"parameter",
        "latex":"\\kappa_\\gamma\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "mejecta_blue":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_{\\rm ejb}\\,(M_\\odot)"
    },
    "mejecta_red":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_{\\rm ejr}\\,(M_\\odot)"
    },
    "vejecta_blue":{
        "kind":"parameter",
        "value":1.0e5,
        "class":"parameter",
        "latex":"v_{\\rm ejb}\\,({\\rm km\\,s}^{-1})"
    },
    "vejecta_red":{
        "kind":"parameter",
        "value":1.0e5,
        "class":"parameter",
        "latex":"v_{\\rm ejr}\\,({\\rm km\\,s}^{-1})"
    },
    "theta":{
	"kind":"parameter",
	"value":0.78,
	"class":"parameter",
	"latex":"\\theta"
    },
    "phi":{
        "kind":"parameter",
        "value":0.78,
        "class":"parameter",
        "latex":"\\phi"
    },
    "rprocess_blue":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "mejecta_blue",
            "frad",
            "texplosion",
            "vejecta_blue",
	    "phi",
	    "theta"
        ],
        "replacements":{
            "mejecta":"mejecta_blue",
            "luminosities":"luminosities_blue",
            "vejecta":"vejecta_blue"
        }
    },
    "diffusion":{
        "kind":"transform",
        "inputs":[
            "rprocess_blue",
            "kappagamma",
            "kappa_blue",
            "mejecta_blue",
            "texplosion",
            "vejecta_blue"
        ],
        "replacements":{
            "luminosities":"luminosities_blue",
            "mejecta":"mejecta_blue",
            "vejecta":"vejecta_blue",
            "kappa":"kappa_blue",
            "kappa_bluegamma":"kappagamma"
        }
    },
    "temperature_floor":{
        "kind":"photosphere",
        "class":"temperature_floor",
        "inputs":[
            "texplosion",
            "temperature_blue",
            "diffusion"
        ],
        "replacements":{
            "luminosities": "luminosities_blue",
            "vejecta":"vejecta_blue",
            "mejecta":"mejecta_blue",
            "kappa":"kappa_blue",
            "temperature":"temperature_blue",
            "temperature_bluephot":"temperaturephot"
        }
    },
    "blackbody":{
        "kind":"sed",
        "inputs":[
            "texplosion",
            "redshift",
            "temperature_floor"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"luminosities_blue"
        }
    },
    "rprocess_red":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "mejecta_red",
            "frad",
            "texplosion",
            "vejecta_red",
	    "theta",
	    "phi"
        ],
        "replacements":{
            "mejecta":"mejecta_red",
            "luminosities":"luminosities_red",
            "vejecta":"vejecta_red"
        }
    },
    "diffusion2":{
        "kind":"transform",
        "class":"diffusion",
        "inputs":[
            "rprocess_red",
            "kappa_red",
            "kappagamma",
            "mejecta_red",
            "texplosion",
            "vejecta_red"
        ],
        "replacements":{
            "luminosities":"luminosities_red",
            "mejecta":"mejecta_red",
            "vejecta":"vejecta_red",
            "kappa":"kappa_red",
            "kappa_redgamma":"kappagamma"
        }
    },
    "temperature_floor2":{
        "kind":"photosphere",
        "class":"temperature_floor",
        "inputs":[
            "texplosion",
            "temperature_red",
            "diffusion2"
        ],
        "replacements":{
            "luminosities": "luminosities_red",
            "vejecta":"vejecta_red",
            "mejecta":"mejecta_red",
            "kappa":"kappa_red",
            "temperature":"temperature_red",
            "temperature_redphot":"temperaturephot"
        }
    },
    "blackbody2":{
        "kind":"sed",
        "class":"blackbody",
        "inputs":[
            "texplosion",
            "redshift",
            "temperature_floor2",
            "blackbody"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"luminosities_red"
        }
    },
    "photometry":{
        "kind":"observable",
        "supports":[
            "ultraviolet",
            "optical",
            "infrared"
        ],
        "inputs":[
            "blackbody2",
            "lumdist"
        ],
        "replacements":{
            "luminosities":"luminosities_blue"
        }
    }
}
