{
    "@references":[
        {
            "bibcode": "2017arXiv170708132V"
        },
        {
            "bibcode": "2017arXiv171011576V"
        }
    ],
   "ebv":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "inputs":[
            "transient"
        ],
        "latex":"E({\\rm B} - {\\rm V})"
    },
    "nhhost":{
        "kind":"parameter",
        "value":1.0e14,
        "class":"parameter",
        "latex":"n_{\\rm H,host}",
        "derived_keys":"avhost"
    },
    "redshift":{
        "kind":"parameter",
        "value":0.009727,
        "class":"parameter",
        "latex":"z"
    },
    "lumdist":{
        "kind":"parameter",
        "value":43.36,
        "class":"parameter",
        "latex":"d"
    },
    "texplosion":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "latex":"t_{\\rm exp}\\,{\\rm (days)}",
        "requests":{
            "min_times": "transient"
        }
    },
    "theta":{
	   "kind":"parameter",
	   "value":0.78,
	   "class":"parameter",
	   "latex":"\\theta"
    },
    "phi":{
        "kind":"parameter",
        "value":0.78,
        "class":"parameter",
        "latex":"\\phi"
    },
    "vk1":{
        "kind":"parameter",
        "value":0.1,
        "class":"parameter",
        "latex":"v_k"
    },
    "xlan1":{
        "kind":"parameter",
        "value":1e-3,
        "class":"parameter",
        "latex":"X_{lan}"
    },
    "mejecta1":{
        "kind":"parameter",
        "value":0.01,
        "class":"parameter",
        "latex":"m"
    },
    "vk0":{
        "kind":"parameter",
        "value":0.1,
        "class":"parameter",
        "latex":"v_k"
    },
    "xlan0":{
        "kind":"parameter",
        "value":1e-3,
        "class":"parameter",
        "latex":"X_{lan}"
    },
    "mejecta0":{
        "kind":"parameter",
        "value":0.01,
        "class":"parameter",
        "latex":"m"
    },
    "mass_weight0":{
        "kind":"parameter",
        "value":1.5,
        "class":"parameter",
        "latex":"m_w"
    },
    "mass_weight1":{
        "kind":"parameter",
        "value":1.5,
        "class":"parameter",
        "latex":"m_w"
    },
    "fakelum":{
        "kind":"engine",
        "inputs": "densetimes"
    },
    "kasen0":{
        "kind":"sed",
        "inputs":[
	        "fakelum",
            "texplosion",
            "redshift",
            "vk0",
            "xlan0",
            "mejecta0",
            "mass_weight0",
            "theta",
            "phi"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "vk":"vk0",
            "xlan":"xlan0",
            "mejecta":"mejecta0",
            "mass_weight":"mass_weight0"
        }
    },
    "kasen1":{
        "kind":"sed",
        "inputs":[
      	    "fakelum",
            "texplosion",
            "redshift",
            "vk1",
            "xlan1",
            "mejecta1",
            "mass_weight1",
            "theta",
            "phi",
            "kasen0"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "vk":"vk1",
            "xlan":"xlan1",
            "mejecta":"mejecta1",
            "mass_weight":"mass_weight1"
        }
    },
    "photometry":{
        "kind":"observable",
        "supports":[
            "ultraviolet",
	         "optical",
            "infrared"
        ],
        "inputs":[
            "kasen1",
            "lumdist"
        ]
}
    }
