{
    "@references":[
        {
            "bibcode": "2017arXiv170708132V"
        },
        {
            "bibcode": "2017arXiv171011576V"
        }
    ],
   "ebv":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "inputs":[
            "transient"
        ],
        "latex":"E({\\rm B} - {\\rm V})"
    },
    "nhhost":{
        "kind":"parameter",
        "value":1.0e14,
        "class":"parameter",
        "latex":"n_{\\rm H,host}",
        "derived_keys":"avhost"
    },
    "redshift":{
        "kind":"parameter",
        "value":0.009727,
        "class":"parameter",
        "latex":"z"
    },
    "lumdist":{
        "kind":"parameter",
        "value":43.36,
        "class":"parameter",
        "latex":"d"
    },
    "texplosion":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "latex":"t_{\\rm exp}\\,{\\rm (days)}",
        "requests":{
            "min_times": "transient"
        }
    },
    "theta":{
	   "kind":"parameter",
	   "value":0.6981317008,
	   "class":"parameter",
	   "latex":"\\theta"
    },
    "phi":{
        "kind":"parameter",
        "value":1.0471975512,
        "class":"parameter",
        "latex":"\\phi"
    },
    "vk1":{
        "kind":"parameter",
        "value":0.24,
        "class":"parameter",
        "latex":"v_c"
    },
    "Msph1":{
        "kind":"parameter",
        "value":0.1,
        "class":"parameter",
        "latex":"m"
    },
    "xlan1":{
        "kind":"parameter",
        "value":1e-1,
        "class":"parameter",
        "latex":"X"
    },
    "vk0":{
        "kind":"parameter",
        "value":0.36,
        "class":"parameter",
        "latex":"v_c"
    },
    "Msph0":{
        "kind":"parameter",
        "value":0.075,
        "class":"parameter",
        "latex":"m"
    },
    "xlan0":{
        "kind":"parameter",
        "value":1e-1,
        "class":"parameter",
        "latex":"X"
    },
    "fakelum":{
        "kind":"engine",
        "inputs": "densetimes"
    },
    "kasen0":{
        "kind":"sed",
        "inputs":[
            "fakelum",
	    "densetimes",
            "texplosion",
            "redshift",
            "theta",
            "phi",
	    "vk0",
	    "xlan0",
	    "Msph0"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "vk":"vk0",
            "xlan":"xlan0",
            "Msph": "Msph0"
        }
    },
    "kasen1":{
        "kind":"sed",
        "inputs":[
      	   "fakelum",
	   "densetimes",
            "texplosion",
            "redshift",
            "theta",
            "phi",
	    "vk1",
	    "Msph1",
	    "xlan1",
            "kasen0"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "vk":"vk1",
            "xlan":"xlan1",
            "Msph":"Msph1"
        }
    },
    "photometry":{
        "kind":"observable",
        "supports":[
            "ultraviolet",
	         "optical",
            "infrared"
        ],
        "inputs":[
            "kasen1",
            "lumdist"
        ]
}
    }
